    "publish-release-next": "nx run tools-release:build-publish-release-next && node dist/libs/tools/publish-release-next/main.js",
    "barista:prerender": "nx run barista-design-system:prerender --routesFile dist/barista-data/routes.txt",
    "workspace-generator": "nx workspace-generator",
    "prepare-husky": "husky install",
    "pre-commit": "nx format:write && git update-index --again",
    "commit-msg": "npx commitlint --edit"
  },
